{
  "name": "vigour-config",
  "version": "0.0.14",
  "author": "Vigour.io <dev@vigour.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/config.git"
  },
  "engines": {
    "node": "^4.1.1"
  },
  "scripts": {
    "test": "gaston test",
    "test-config": "gaston test -r node -s test/node/config",
    "doc": "node documentation.js",
    "precommit": "./node_modules/.bin/standard lib/**/*.js"
  },
  "browserify": {
    "./lib/util/findPackage/index.js": "./lib/util/findPackage/browser.js"
  },
  "description": "Observable configuration",
  "bugs": {
    "url": "https://github.com/vigour-io/config/issues"
  },
  "dependencies": {
    "vigour-js": "0.5.3",
    "lodash": "3.10.x"
  },
  "main": "./lib",
  "homepage": "https://github.com/vigour-io/config",
  "keywords": [
    "javascript",
    "framework",
    "browserify",
    "observable",
    "crossplatform"
  ],
  "contributors": [
    "Jim de beer <jim@vigour.io>",
    "Marcus Besjes <marcus@vigour.io>",
    "Shawn Inder <shawn@vigour.io>"
  ],
  "license": "MIT",
  "gaston": {
    "source-maps": true
  },
  "vigour": {
    "services": {
      "sentinel": {
        "branches": [],
        "unignoreFolders": []
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "sinon",
      "expect",
      "xit"
    ]
  },
  "devDependencies": {
    "husky": "^0.10.2",
    "standard": "^5.4.1"
  }
}
